
record(mbbi,"${ECMC_P}${sourceName}"){
    field(DESC, "$(DESC=)" )
    field(PINI, "$(PINI=1)")
    field(VAL,  "$(VAL=0)")
    field(DTYP, "$(DTYP=asynUInt32Digital)")
    field(INP,  "@asynMask($(PORT),$(ADDR=0),$(MASK=0xFFFFFFFF),$(TIMEOUT=1))T_SMP_MS=${T_SMP_MS=1000}/TYPE=$(DTYP=asynUInt32Digital)/ec${M_ID}.s${S_ID}.${sourceName}?")
    field(SCAN, "I/O Intr")
    field(TSE,  "$(TSE=-2)")
    field(ZRST, "${ZRST=zero}")
    field(ONST, "${ONST=one}")
    field(TWST, "${TWST=two}")
    field(THST, "${THST=three}")
    field(FRST, "${FRST=four}")
    field(FVST, "${FVST=five}")
    field(SXST, "${SXST=six}")
    field(SVST, "${SVST=seven}")
    field(EIST, "${EIST=eight}")
    field(NIST, "${NIST=nine}")
    field(TEST, "${TEST=ten}")
    field(ELST, "${ELST=eleven}")
    field(TVST, "${TVST=twelve}")
    field(TTST, "${TTST=thirteen}")
    field(FTST, "${FTST=forteen}")    
    field(FFST, "${FFST=fifteen}")    
    field(ZRVL, "${ZRVL=0x0}")
    field(ONVL, "${ONVL=0x1}")
    field(TWVL, "${TWVL=0x2}")
    field(THVL, "${THVL=0x3}")
    field(FRVL, "${FRVL=0x4}")
    field(FVVL, "${FVVL=0x5}")
    field(SXVL, "${SXVL=0x6}")
    field(SVVL, "${SVVL=0x7}")
    field(EIVL, "${EIVL=0x8}")
    field(NIVL, "${NIVL=0x9}")
    field(TEVL, "${TEVL=0xA}")
    field(ELVL, "${ELVL=0xB}")
    field(TVVL, "${TVVL=0xC}")
    field(TTVL, "${TTVL=0xD}")
    field(FTVL, "${FTVL=0xE}")    
    field(FFVL, "${FFVL=0xF}")
}
